ht blue'\">Update</v-btn>\n\t\t\t\t\t\t\t\t\t<v-btn small color=\"error\">Delete</v-btn>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</td>\n\t\t\t\t\t\t</tr>\n\t\t\t\t\t</tbody>\n\t\t\t\t</v-simple-table>\n\t\t\t</v-col>\n\t\t</v-row>\n\t</v-container>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n\tdata: function(){\n\t\treturn {\n\t\t\tname: 'Books',\n\t\t\tbooks: [],\n\t\t}\n\t},\n\tmethods: {\n\t\tshowAddBook: function() {\n\t\t\tthis.$store.commit('change_visibility', true)\n\t\t},\n\t\tgetBooks() {\n\t\tconst path = 'http://127.0.0.1:5000/books';\n\t\taxios.get(path)\n\t\t\t.then((res) => {\n\t\t\tthis.books = res.data.books;\n\t\t\t})\n\t\t\t.catch((error) => {\n\t\t\t// eslint-disable-next-line\n\t\t\tconsole.error(error);\n\t\t\t});\n\t\t},\n\t},\n\tcreated() {\n\t\tthis.getBooks();\n\t},\n};\n</script>\n\t\n\t"]}]}