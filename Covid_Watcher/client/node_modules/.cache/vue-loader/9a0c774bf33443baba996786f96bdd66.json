\n    \n</template>\n// Title, Author, Have you read it?, submit, reset\n\n<script>\nimport axios from 'axios'\n\nexport default {\n\tdata: function() {\n\t\treturn {\n\t\t\tname: \"BooksModal\",\n\t\t\tform: {\n\t\t\t\ttitle: '',\n\t\t\t\tauthor: '',\n\t\t\t\tread: false,\n\t\t\t},\n\t\t\tdialog: false,\n\t\t}\n\t},\n\tmethods: {\n\t\taddBook(payload) {\n\t\t\tconst path = 'http://localhost:5000/books';\n\t\t\taxios.post(path, payload)\n\t\t\t\t.then(() => {\n\t\t\t\tthis.getBooks();\n\t\t\t\t})\n\t\t\t\t.catch((error) => {\n\t\t\t\t// eslint-disable-next-line\n\t\t\t\tconsole.log(error);\n\t\t\t\tthis.getBooks();\n\t\t\t\t});\n\t\t},\n\t\tinitForm() {\n\t\t\tthis.form.title = '';\n\t\t\tthis.form.author = '';\n\t\t\tthis.form.read = [];\n\t\t},\n\t\tonSubmit(evt) {\n\t\t\tevt.preventDefault();\n\t\t\tthis.$refs.addBookModal.hide();\n\t\t\tlet read = false;\n\t\t\tif (this.form.read[0]) read = true;\n\t\t\tconst payload = {\n\t\t\t\ttitle: this.form.title,\n\t\t\t\tauthor: this.form.author,\n\t\t\t\tread, // property shorthand\n\t\t\t};\n\t\t\tthis.addBook(payload);\n\t\t\tthis.initForm();\n\t\t\tthis.$store.commit('change_visibility', false)\n\t\t},\n\t\tonReset(evt) {\n\t\t\tevt.preventDefault();\n\t\t\tthis.$refs.addBookModal.hide();\n\t\t\tthis.initForm();\n\t\t},\n\tcreated() {\n\t\tthis.getBooks();\n\t\t},\n\t}\n};\n</script>"]}]}