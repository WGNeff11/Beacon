;\n  border: 2px solid $buttonColor;\n\tcursor: pointer;\n\tcolor: #ffffff;\n\tfont-size: 16px;\n\tcolor: $buttonColor;\n\toutline: none;\n  text-align: center;\n  \n  &:hover,\n  &:focus { \n    border-color: var(--hover);\n    background-color: var(--hover);\n    color: #f8f8f8;\n  }\n  &:active {\n\t\tfont-size: 15px;\n\t\ttransform: translateY(1px);\n\t}\n  &.active {\n\t\tfont-size: 0;\n\t\tborder-radius: 50%;\n    min-width: 50px;\n    max-width: 50px;\n    min-height: 50px;\n    max-height:50px;\n\t\tbackground: transparent;\n  }\n  &.loader {\n\t\tborder-right: 2px solid #ffffff;\n\t\tanimation: loader .4s linear infinite;\n\t}\n\t// &.success {\n\t// \tbackground: #F8F8F8;\n\t// \tborder-color: #f8f8f8;\n\t// \tfont-size: 14px;\n\t// \tcolor: $buttonColor;\n  // }\n  \n}\n@keyframes loader {\n\t0% {\n\t\ttransform: rotateZ(0);\n\t}\n\t100% {\n\t\ttransform: rotateZ(360deg);\n\t}\n}\n\n#submit-button .submit-icon {\n  color: #41EA96;\n  transition: 0.5s ease-in;\n}\n#submit-button:hover .submit-icon {\n  color: #f8f8f8;\n}\n#submit-button:focus .submit-icon {\n  color: #f8f8f8;\n}\n\n.left-side-card {\n  padding:3%;\n}\n// #submit-button {\n//   display: flex;\n//   align-content: center;\n//   background: none;\n//   border: 2px solid;\n//   font: inherit;\n//   line-height: 1;\n//   font-size: 16pt;\n//   margin: 0.5em;\n//   padding: 0.5em 0.5em;\n// }\n\n  /* #submit-button:hover {\n    background-color: #8C8EF2;\n    color: #F8F8F8;\n\n  } */\n#submit-button:hover,\n#submit-button:focus {\n  box-shadow: inset 15.5em 0 0 0 var(--hover);\n}\n \n\n.button-background {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  min-height: 120px;\n  min-width: 70px;\n}\n\n#big-button {\n  padding: 0;\n  margin: 0;\n  font-family: \"Oswald\", sans-serif;\n  // display: flex;\n  // align-items: center;\n  // justify-content: center;\n  min-width: 100%;\n  text-transform: none !important;\n  font-weight: bold;\n  font-size: 20pt;\n  background-color: #8889EA;\n  color: #F8F8F8;\n\n}\n\n.button-row {\n  padding-bottom: 3%;\n}\n\n</style>"]}]}