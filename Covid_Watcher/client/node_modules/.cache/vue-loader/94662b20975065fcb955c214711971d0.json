xpert\n                  </v-btn>\n                </v-col>\n              </v-row>\n            </div>\n            </v-col>\n          </v-row>\n          <v-row>\n            <v-col>\n              <div data-aos=\"fade-up\">\n                <ExpertTable></ExpertTable>\n              </div>\n            </v-col>\n          </v-row>\n        </v-col>\n          <v-col v-if=\"show_form\" cols=\"6\">\n            <v-card flat class=\"expert-form\">\n              <v-row align-center>\n                <v-col>\n                  <v-form\n                  v-model=\"valid\"\n                  class=\"justify-center\"\n                >\n                  <v-text-field\n                    v-model=\"name\"\n                    label=\"Name\"\n                    required\n                    class=\"form-input\"\n                    color=\"#00ACEE\"\n                  ></v-text-field>\n\n                  <v-text-field\n                  v-model=\"twitterHandle\"\n                  class=\"form-input\"\n                  label=\"Twitter Handle\"\n                  required\n                  color=\"#00ACEE\"\n                ></v-text-field>\n\n                  <v-text-field\n                    v-model=\"expertise\"\n                    label=\"Expertise\"\n                    class=\"form-input\"\n                    color=\"#00ACEE\"\n                  >\n                  </v-text-field>\n                  <v-card-actions class=\"justify-center\">\n                    <v-btn\n                      type=\"button\"\n                      class=\"form-input slide\"\n                      id=\"submit-button\"\n                      depressed\n                      tile\n                      :class=\"{'active': active, 'successful': success, 'loader': loader, 'animated': animated, 'pulse': pulse}\"\n                      @click=\"animate()\"\n                      \n                      >\n                        {{btn_text}}\n                        <v-icon right c